{
  "name": "wiz",
  "displayName": "wiz",
  "description": "Wiz language support for Visual Studio Code",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "wiz",
      "aliases": ["wiz", "wiz"],
      "extensions": [".wiz"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "wiz",
      "scopeName": "source.wiz",
      "path": "./syntaxes/wiz.tmLanguage.json"
    }]
  }
}
