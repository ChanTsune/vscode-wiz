{
  "fileTypes": [],
  "injectionSelector": "L:markup.fenced_code.block.markdown",
  "patterns": [
    {
      "include": "#wiz-code-block"
    }
  ],
  "repository": {
    "wiz-code-block": {
      "begin": "wiz",
